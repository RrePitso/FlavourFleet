{
  "version": 3,
  "name": "flavourfleet",
  "rootDirectory": "client",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "VITE_FIREBASE_API_KEY": "",
    "VITE_FIREBASE_PROJECT_ID": "",
    "VITE_FIREBASE_APP_ID": ""
  },
  "build": {
    "env": {
      "VITE_FIREBASE_API_KEY": "",
      "VITE_FIREBASE_PROJECT_ID": "",
      "VITE_FIREBASE_APP_ID": ""
    }
  },
  "functions": {},
  "node": {
    "version": "22.x"
  }
}




